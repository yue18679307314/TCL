<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kuyu.mapper.PcmsSupplierMapper">


    <insert id="insertPcmsSupplier" parameterType="com.kuyu.vo.PcmsSupplierVo">
		INSERT INTO
		pcms_supplier(spid,spname,sptype,company,del_flag,del_comp,person_id,create_time)
		VALUES
		(#{vendor_id},#{vendor_name},#{type},#{company},#{del_flag},#{del_comp},#{person_id},#{create_time})
	</insert>

</mapper>