<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kuyu.mapper.pcms.PcmsTovoidItemMapper">

    <select id="selectdetailByItid" resultType="com.kuyu.vo.pcms.PcmsTovoidItemVo">
        SELECT t.id,t.context,t.itid,t.create_time,t.operator FROM pcms_tovoid_item t
        <where>
            <if test="itid != null and itid != ''">
                t.itid = #{itid}
            </if>
        </where>
    </select>

</mapper>